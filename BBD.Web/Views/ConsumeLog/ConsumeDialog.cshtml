@{
    ViewBag.Title = "结束消费记录";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model BBD.Models.tb_Consume_Log
<form id="dialogForm" method="post" action="#">
    <input type="hidden" id="Id" value="@Model.Id" name="Id" />
    <input type="hidden" id="uId" name="uId" value="@Model.uId" />
    <table style="width: 100%; text-align: center">
        <tr>
            <td>
                <label for="flag">
                    类型:
                </label>
            </td>
            <td>
               @if (Model.flag==0)
               {
                   <text>仪器</text>
               }
               else if (Model.flag==1)
               {
                   <text>点穴</text>
               }
            </td>
        </tr>
        <tr>
            <td>
                <label for="num">
                    次数:
                </label>
            </td>
            <td>
                @Model.num
            </td>
        </tr>
        <tr>
            <td>
                <label for="rec_date">
                    创建日期:
                </label>
            </td>
            <td>
                @Convert.ToDateTime(Model.rec_date).ToString("yyyy-MM-dd")
            </td>
        </tr>

        <tr>
            <td>
                <label for="finish_time">
                    完成时间:
                </label>
            </td>
            <td>
                <input id="finish_time" class="easyui-timespinner" style="width:80px;" value="@Model.finish_time" name="finish_time">
                @*<input id="finish_time" class="easyui-datetimebox" type="text" name="finish_time" value="@Model.finish_time" data-options="onshowpanel:function(){$(this).datetimebox('spinner').timespinner('setvalue','00:00:00'); }" />*@
            </td>
        </tr>

    </table>
    <script type="text/javascript">
  
    </script>
</form>
